    "thermal_breaks": {
      "roof_thermal_breaks": {
        "condition": "J3D5 (Roof Thermal Breaks)",
        "requirements": [
          "Roofs with metal sheet roofing fixed directly to metal purlins/rafters/battens (with no ceiling lining or lining fixed directly) must have a thermal break (≥ R0.2) installed between roofing and support.",
          "Does not apply to roofs constructed using insulated sandwich panels."
        ]
      },
      "wall_thermal_breaks": {
        "condition": "J3D6 (Wall Thermal Breaks)",
        "requirements": [
          "Metal-framed envelope walls clad with weatherboards, fibre-cement, metal sheeting etc. (with no wall lining or lining fixed directly to the same frame) must have a thermal break (≥ R0.2) installed at all points of contact between cladding and frame.",
          "Does not apply to walls constructed using insulated sandwich panels."
        ]
      }
    },
    "floor_requirements": {
      "condition": "J3D10 (Floors - Specifics)",
      "requirements": [
        "Concrete slab-on-ground with in-slab/in-screed heating/cooling requires R1.0 vertical edge insulation.",
        "Required edge insulation must be water-resistant and continuous to specified depth.",
        "Requirement for edge insulation does not apply to in-screed systems solely in bathrooms/amenity areas."
      ]
    },
